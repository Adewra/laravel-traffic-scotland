    "require": {
        "laravel/laravel": "^5.7",
        "arandilopez/laravel-feed-parser": "^0.1.3",
        "dmore/chrome-mink-driver": "^2.0@dev",
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "~3.0",
        "behat/mink": "^1.7@dev"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Ally Dewar",
            "email": "Alastair@adewra.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Adewra\\TrafficScotland\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adewra\\TrafficScotland\\Test\\": "tests/"
        }
    }
}
